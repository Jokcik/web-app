{
  "name": "web-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prod": "nodemon -L -e ts,json --exec ts-node --ignoreWarnings 2307 src/server.ts",
    "start": "ts-node src/server.ts",
    "start:backup": "ts-node src/server.ts 9090",
    "build": "ng build --prod -op dist2 && rm -rf dist && mv dist2 dist && npm run css",
    "build:start": "ng build --prod -op dist2 && rm -rf dist && mv dist2 dist && npm run css && npm start",
    "node": "ts-node src/server.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "css": "node-sass --output-style compressed public/static/sass/style-new.scss --output public/static/css",
    "css2": "node-sass --output-style expanded public/static/sass/style-new.scss public/static/css/style-new.css  --source-map public/static/css/style-new.css.map && postcss --use autoprefixer --autoprefixer.browsers 'latest 2 versions' -o public/static/css/style-new.min.css < public/static/css/style-new.css && cleancss public/static/css/style-new.css -o public/static/css/style-new.min.css",
    "csswatch": "node-sass --watch --output-style expanded public/static/sass/style-new.scss public/static/css/style-new.css  --source-map public/static/css/style-new.css.map && postcss --use autoprefixer --autoprefixer.browsers 'latest 2 versions' -o public/static/css/style-new.min.css < public/static/css/style-new.css && cleancss public/static/css/style-new.css -o public/static/css/style-new.min.css",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.2.0",
    "@angular/cli": "^6.0.7",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@angular/material": "^6.2.0",
    "@angular/material-moment-adapter": "^6.2.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/platform-server": "^5.2.1",
    "@angular/router": "^6.0.3",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "@types/express": "^4.0.39",
    "@types/lodash": "^4.14.91",
    "@types/mv": "^2.1.0",
    "angular2-universal": "^2.1.0-rc.1",
    "body-parser": "^1.18.2",
    "core-js": "^2.5.1",
    "express": "^4.16.2",
    "express-cors": "0.0.3",
    "express-formidable": "^1.0.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "ngx-ckeditor": "^0.1.1",
    "moment": "^2.22.1",
    "ngx-gallery": "^4.1.2",
    "ngx-infinite-scroll": "^0.8.3",
    "ngx-resource": "^3.6.3",
    "node-sass": "^4.7.2",
    "serve-static": "^1.13.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "ts-loader": "^3.2.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.7.2",
    "typings": "^2.1.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@types/jasmine": "^2.6.3",
    "@types/jasminewd2": "^2.0.3",
    "codelyzer": "^4.3.0",
    "eslint-plugin-import": "^2.12.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.2.0",
    "stylelint": "^9.1.1",
    "stylelint-config-recommended-scss": "^3.1.0",
    "stylelint-scss": "^2.4.0",
    "tslint": "^5.8.0",
    "autoprefixer": "^7.2.5",
    "postcss-cli": "^5.0.0",
    "clean-css-cli": "^4.1.10",
    "@angular-devkit/build-angular": "~0.6.6",
    "@angular-devkit/build-optimizer": "^0.6.7"
  }
}
