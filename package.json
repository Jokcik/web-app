{
  "name": "web-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "nodemon -L -e ts,json --exec ts-node src/server.ts",
    "build": "ts-node src/server.ts",
    "prod": "ng build -op dist2 && rm -rf dist && mv dist2 dist_main && npm run css && ts-node src/server.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "css": "node-sass --output-style compressed public/static/sass/style-new.scss --output public/static/css",
    "csswatch": "node-sass --output-style compressed public/static/sass/style-new.scss --output public/static/css --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0-rc.1",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@nestjs/common": "^4.2.1",
    "@nestjs/core": "^4.2.1",
    "@nestjs/microservices": "^4.2.1",
    "@nestjs/testing": "^4.2.1",
    "@nestjs/websockets": "^4.2.1",
    "@types/express": "^4.0.39",
    "angular4-carousel": "^3.1.8",
    "body-parser": "^1.17.2",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "express-cors": "0.0.3",
    "hammerjs": "^2.0.8",
    "mongoose": "^4.13.0",
    "ng2-ckeditor": "^1.1.9",
    "ngx-resource": "^3.6.3",
    "node-sass": "^4.6.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "serve-static": "^1.13.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
